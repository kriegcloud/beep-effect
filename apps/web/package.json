    "dev": "NODE_OPTIONS='--inspect' bun run dotenvx -- next dev --turbopack",
    "dev:https": "bun run dotenvx -- bunx next dev --turbopack --experimental-https",
    "start": "bun run dotenvx -- next start",
    "build": "bun run dotenvx -- next build --turbopack",
    "build:turbo": "bun run dotenvx -- next build --turbopack",
    "clean": "rm -rf node_modules .next out dist build .turbo .tsbuildinfo",
    "lint": "biome check .",
    "test": "bun test",
    "lint:fix": "biome check . --write",
    "check": "tsc --noEmit",
    "dotenvx": "dotenvx run -f ../../.env --",
    "lint:circular": "bunx madge -c .",
    "execute": "bun run dotenvx -- bunx tsx ./execute.ts"
  },
