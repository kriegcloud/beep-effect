{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "exactOptionalPropertyTypes": false,
    "moduleDetection": "force",
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowImportingTsExtensions": false,
    "isolatedModules": true,
    "noImplicitReturns": false,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "noEmitOnError": false,
    "noErrorTruncation": false,
    "noEmit": true,
    "allowJs": false,
    "checkJs": false,
    "forceConsistentCasingInFileNames": true,
    "useDefineForClassFields": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "resolveJsonModule": true,
    "strict": true,
    "strictNullChecks": true,
    "removeComments": false,
    "types": ["node"],
    "outDir": "build/src",
    "tsBuildInfoFile": ".tsbuildinfo/src.tsbuildinfo",
    "baseUrl": ".",
    "jsx": "preserve",
    "module": "ESNext",
    "target": "ES2022",
    "lib": ["ES2024", "DOM", "DOM.Iterable"],
    "moduleResolution": "bundler",
    "incremental": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "paths": {
      "@/*": ["./src/*"],
      "@beep/ui": ["../../packages/ui/src/index"],
      "@beep/ui/*": ["../../packages/ui/src/*"],
      "@beep/utils": ["../../packages/common/utils/src/index"],
      "@beep/utils/*": ["../../packages/common/utils/src/*"],
      "@beep/types": ["../../packages/common/types/src/index"],
      "@beep/types/*": ["../../packages/common/types/src/*"],
      "@beep/invariant": ["../../packages/common/invariant/src/index"],
      "@beep/invariant/*": ["../../packages/common/invariant/src/*"],
      "@beep/schema": ["../../packages/common/schema/src/index"],
      "@beep/schema/*": ["../../packages/common/schema/src/*"],
      "@beep/constants": ["../../packages/common/constants/src/index"],
      "@beep/constants/*": ["../../packages/common/constants/src/*"],
      "@beep/logos": ["../../packages/common/logos/src/index"],
      "@beep/logos/*": ["../../packages/common/logos/src/*"],
      "@beep/rete": ["../../packages/common/rete/src/index"],
      "@beep/rete/*": ["../../packages/common/rete/src/*"],
      "@beep/errors/shared": ["../../packages/common/errors/src/shared"],
      "@beep/errors/client": ["../../packages/common/errors/src/client"],
      "@beep/errors/server": ["../../packages/common/errors/src/server"],
      "@beep/env/client": ["../../packages/env/src/client"],
      "@beep/env/server": ["../../packages/env/src/server"],
      "@beep/files-application": ["../../packages/files/application/src/index"],
      "@beep/files-application/*": ["../../packages/files/application/src/*"],
      "@beep/files-infra": ["../../packages/files/infra/src/index"],
      "@beep/files-infra/*": ["../../packages/files/infra/src/*"],
      "@beep/files-ui": ["../../packages/files/ui/src/index"],
      "@beep/files-ui/*": ["../../packages/files/ui/src/*"],
      "@beep/better-auth": ["../../packages/adapters/better-auth/src/index"],
      "@beep/better-auth/*": ["../../packages/adapters/better-auth/src/*"],
      "@beep/shared-domain": ["../../packages/shared/domain/src/index"],
      "@beep/shared-domain/*": ["../../packages/shared/domain/src/*"],
      "@beep/runtime/client": ["../../packages/runtime/src/client"],
      "@beep/runtime/server": ["../../packages/runtime/src/server"],
      "@beep/xstate": ["../../packages/adapters/xstate/src/index"],
      "@beep/xstate/*": ["../../packages/adapters/xstate/src/*"]
    },
    "plugins": [
      {
        "name": "next"
      },
      {
        "name": "@effect/language-service"
      }
    ]
  },
  "references": [
    {
      "path": "../../packages/adapters/xstate/tsconfig.build.json"
    },
    {
      "path": "../../packages/adapters/better-auth/tsconfig.build.json"
    },
    {
      "path": "../../packages/ui/tsconfig.build.json"
    },
    {
      "path": "../../packages/env/tsconfig.build.json"
    },
    {
      "path": "../../packages/common/utils/tsconfig.build.json"
    },
    {
      "path": "../../packages/common/types/tsconfig.build.json"
    },
    {
      "path": "../../packages/common/invariant/tsconfig.build.json"
    },
    {
      "path": "../../packages/common/schema/tsconfig.build.json"
    },
    {
      "path": "../../packages/common/constants/tsconfig.build.json"
    },
    {
      "path": "../../packages/common/rete/tsconfig.build.json"
    },
    {
      "path": "../../packages/common/logos/tsconfig.build.json"
    },
    {
      "path": "../../packages/common/errors/tsconfig.build.json"
    },
    {
      "path": "../../packages/env/tsconfig.build.json"
    },
    {
      "path": "../../packages/files/application/tsconfig.build.json"
    },
    {
      "path": "../../packages/files/infra/tsconfig.build.json"
    },
    {
      "path": "../../packages/files/ui/tsconfig.build.json"
    },
    {
      "path": "../../packages/shared/domain/tsconfig.build.json"
    },
    {
      "path": "../../packages/runtime/tsconfig.build.json"
    }
  ],
  "include": [
    "./scripts/*.ts",
    "execute.ts",
    "next-env.d.ts",
    "next.config.ts",
    "*.json",
    "src/**/*.json",
    ".",
    ".next/types/**/*.ts"
  ],
  "exclude": ["vitest.config.ts"]
}
