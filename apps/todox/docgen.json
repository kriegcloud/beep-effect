{
  "$schema": "../../node_modules/@effect/docgen/schema.json",
  "srcDir": "src",
  "outDir": "docs",
  "srcLink": "https://github.com/kriegcloud/beep-effect/tree/main/apps/todox/src/",
  "exclude": ["src/internal/**/*.ts"],
  "examplesCompilerOptions": {
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "module": "ES2024",
    "target": "ES2024",
    "lib": ["ES2024", "DOM", "DOM.Iterable"],
    "paths": {
      "@beep/todox": ["./src/index.ts"],
      "@beep/todox/*": ["./src/*"],
      "@beep/errors/client": ["../../packages/common/errors/src/client.js"],
      "@beep/errors/shared": ["../../packages/common/errors/src/shared.js"],
      "@beep/errors/server": ["../../packages/common/errors/src/server.js"],
      "@beep/workspaces-server": [
        "../../packages/workspaces/server/src/index.js"
      ],
      "@beep/workspaces-server/*": ["../../packages/workspaces/server/src/*.js"],
      "@beep/iam-domain": ["../../packages/iam/domain/src/index.js"],
      "@beep/iam-domain/*": ["../../packages/iam/domain/src/*.js"],
      "@beep/iam-server": ["../../packages/iam/server/src/index.js"],
      "@beep/iam-server/*": ["../../packages/iam/server/src/*.js"],
      "@beep/iam-client": ["../../packages/iam/client/src/index.js"],
      "@beep/iam-client/*": ["../../packages/iam/client/src/*.js"],
      "@beep/runtime-client": ["../../packages/runtime/client/src/index.js"],
      "@beep/runtime-client/*": ["../../packages/runtime/client/src/*.js"],
      "@beep/runtime-server": ["../../packages/runtime/server/src/index.js"],
      "@beep/runtime-server/*": ["../../packages/runtime/server/src/*.js"],
      "@beep/shared-env": ["../../packages/shared/env/src/index.js"],
      "@beep/shared-env/*": ["../../packages/shared/env/src/*.js"],
      "@beep/invariant": ["../../packages/common/invariant/src/index.js"],
      "@beep/invariant/*": ["../../packages/common/invariant/src/*.js"],
      "@beep/schema": ["../../packages/common/schema/src/index.js"],
      "@beep/schema/*": ["../../packages/common/schema/src/*.js"],
      "@beep/utils": ["../../packages/common/utils/src/index.js"],
      "@beep/utils/*": ["../../packages/common/utils/src/*.js"],
      "@beep/constants": ["../../packages/common/constants/src/index.js"],
      "@beep/constants/*": ["../../packages/common/constants/src/*.js"],
      "@beep/shared-domain": ["../../packages/shared/domain/src/index.js"],
      "@beep/shared-domain/*": ["../../packages/shared/domain/src/*.js"],
      "@beep/identity": ["../../packages/common/identity/src/index.js"],
      "@beep/identity/*": ["../../packages/common/identity/src/*.js"],
      "@beep/build-utils": ["../../tooling/build-utils/src/index.js"],
      "@beep/build-utils/*": ["../../tooling/build-utils/src/*.js"],
      "@beep/types": ["../../packages/common/types/src/index.js"],
      "@beep/types/*": ["../../packages/common/types/src/*.js"]
    }
  }
}
