{
  "extends": "../../tsconfig.base.jsonc",
  "include": ["test", "test/**/*.json", "test/*.*.json"],
  "references": [
    {
      "path": "../../packages/common/utils/tsconfig.build.json"
    },
    {
      "path": "../../packages/common/schema/tsconfig.build.json"
    },
    {
      "path": "../../packages/common/constants/tsconfig.build.json"
    },
    {
      "path": "../../packages/shared/env"
    },
    {
      "path": "../../packages/runtime/server/tsconfig.build.json"
    },
    {
      "path": "../../packages/runtime/client/tsconfig.build.json"
    }
  ],
  "compilerOptions": {
    "types": ["node", "bun"],
    "rootDir": "test",
    "resolveJsonModule": true,
    "noEmit": true,
    "paths": {
      "*": ["./*"],
      "@/*": ["./src/*"],
      "@beep/types": ["../../packages/common/types/src/index"],
      "@beep/types/*": ["../../packages/common/types/src/*"],
      "@beep/invariant": ["../../packages/common/invariant/src/index"],
      "@beep/invariant/*": ["../../packages/common/invariant/src/*"],
      "@beep/utils": ["../../packages/common/utils/src/index"],
      "@beep/utils/*": ["../../packages/common/utils/src/*"],
      "@beep/schema": ["../../packages/common/schema/src/index"],
      "@beep/schema/*": ["../../packages/common/schema/src/*"],
      "@beep/constants": ["../../packages/common/constants/src/index"],
      "@beep/constants/*": ["../../packages/common/constants/src/*"],
      "@beep/errors/client": ["../../packages/common/errors/src/client"],
      "@beep/errors/shared": ["../../packages/common/errors/src/shared"],
      "@beep/errors/server": ["../../packages/common/errors/src/server"],
      "@beep/shared-domain": ["../../packages/shared/domain/src/index"],
      "@beep/shared-domain/*": ["../../packages/shared/domain/src/*"],
      "@beep/shared-tables": ["../../packages/shared/tables/src/index"],
      "@beep/shared-tables/*": ["../../packages/shared/tables/src/*"],
      "@beep/shared-server": ["../../packages/shared/server/src/index"],
      "@beep/shared-server/*": ["../../packages/shared/server/src/*"],
      "@beep/shared-client": ["../../packages/shared/client/src/index"],
      "@beep/shared-client/*": ["../../packages/shared/client/src/*"],
      "@beep/iam-domain": ["../../packages/iam/domain/src/index"],
      "@beep/iam-domain/*": ["../../packages/iam/domain/src/*"],
      "@beep/iam-tables": ["../../packages/iam/tables/src/index"],
      "@beep/iam-tables/*": ["../../packages/iam/tables/src/*"],
      "@beep/iam-server": ["../../packages/iam/server/src/index"],
      "@beep/iam-server/*": ["../../packages/iam/server/src/*"],
      "@beep/iam-client": ["../../packages/iam/client/src/index"],
      "@beep/iam-client/*": ["../../packages/iam/client/src/*"],
      "@beep/documents-domain": ["../../packages/documents/domain/src/index"],
      "@beep/documents-domain/*": ["../../packages/documents/domain/src/*"],
      "@beep/documents-tables": ["../../packages/documents/tables/src/index"],
      "@beep/documents-tables/*": ["../../packages/documents/tables/src/*"],
      "@beep/documents-server": ["../../packages/documents/server/src/index"],
      "@beep/documents-server/*": ["../../packages/documents/server/src/*"],
      "@beep/documents-client": ["../../packages/documents/client/src/index"],
      "@beep/documents-client/*": ["../../packages/documents/client/src/*"],
      "@beep/runtime-server": ["../../packages/runtime/server/src/index"],
      "@beep/runtime-server/*": ["../../packages/runtime/server/src/*"],
      "@beep/runtime-client": ["../../packages/runtime/client/src/index"],
      "@beep/runtime-client/*": ["../../packages/runtime/client/src/*"]
    },
    "outDir": "build/test"
  }
}
