{
  "name": "@beep/notes",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "analyze": "ANALYZE=true bun run build",
    "build": "bun run dotenvx -- next build",
    "build:turbo": "next build --turbopack",
    "build:registry": "tsx --tsconfig ./scripts/tsconfig.scripts.json scripts/build-registry.mts",
    "build:tw": "bun run tailwindcss -i ./src/app/globals.css -o ./public/css/tailwind.css --minify",
    "check": "tsc --noEmit",
    "db:deploy": "bun run dotenvx -- prisma migrate deploy",
    "db:generate": "bun run dotenvx -- prisma generate",
    "db:migrate:squash": "rm -rf prisma/migrations && bun run dotenvx -- prisma migrate dev --name init",
    "db:push": "bun run dotenvx -- prisma db push --skip-generate",
    "db:reset": "bun run dotenvx -- prisma migrate reset --force && bun run dotenvx -- prisma db push --accept-data-loss",
    "db:script": "bun run dotenvx -- tsx prisma/scripts/script.ts",
    "db:seed": "bun run dotenvx -- prisma db seed",
    "db:studio": "bun run dotenvx -- prisma studio",
    "dev": "run-p dev:*",
    "dev:app": "NODE_OPTIONS='--inspect=9232' bun run dotenvx -- next dev --turbopack",
    "dev:https": "bun run dotenvx -- bunx next dev --turbopack --experimental-https",
    "dev:yjs": "bun run dotenvx -- tsx watch src/server/yjs/server.ts",
    "dev:db": "docker-compose up",
    "docker": "docker-compose -f docker-compose.web.yml --env-file .env.local up --build",
    "dotenvx": "dotenvx run -f .env.local --",
    "postinstall": "bun run db:generate",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "lint:circular": "bunx madge -c .",
    "format": "biome format --write .",
    "migrate": "bun run dotenvx -- prisma migrate dev",
    "nuke": "rimraf '**/node_modules'",
    "scheduled": "node tooling/scripts/clearFiles.js",
    "start": "bun run dotenvx -- run-p start:*",
    "start:app": "bun run dotenvx -- next start --port 3000",
    "start:yjs": "bun run dotenvx -- tsx src/server/yjs/server.ts",
    "sync:plate": "node tooling/scripts/sync-plate.cjs",
    "sync:app-ui": "node tooling/scripts/sync-app-ui.cjs",
    "test": "bun test",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch",
    "vendor": "sh tooling/scripts/vendor.sh"
  },
