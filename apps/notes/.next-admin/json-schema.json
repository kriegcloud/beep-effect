{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "Session": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "expires_at": {
          "type": "string",
          "format": "date-time"
        },
        "user": {
          "$ref": "#/definitions/User"
        },
        "ip_address": {
          "type": ["string", "null"]
        },
        "user_agent": {
          "type": ["string", "null"]
        }
      }
    },
    "OauthAccount": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "providerId": {
          "type": "string"
        },
        "providerUserId": {
          "type": "string"
        },
        "user": {
          "$ref": "#/definitions/User"
        }
      }
    },
    "User": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "username": {
          "type": "string"
        },
        "password_hash": {
          "type": ["string", "null"]
        },
        "email": {
          "type": ["string", "null"]
        },
        "sessions": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Session"
          }
        },
        "oauthAccounts": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/OauthAccount"
          }
        },
        "role": {
          "type": "string",
          "default": "USER",
          "enum": ["USER", "ADMIN", "SUPERADMIN"]
        },
        "uploadLimit": {
          "type": "integer",
          "default": 100000000
        },
        "name": {
          "type": ["string", "null"]
        },
        "firstName": {
          "type": ["string", "null"]
        },
        "lastName": {
          "type": ["string", "null"]
        },
        "profileImageUrl": {
          "type": ["string", "null"]
        },
        "version": {
          "type": "integer",
          "default": 1
        },
        "stripeCustomerId": {
          "type": ["string", "null"]
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "updatedAt": {
          "type": "string",
          "format": "date-time"
        },
        "deletedAt": {
          "type": ["string", "null"],
          "format": "date-time"
        },
        "documents": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Document"
          }
        },
        "comments": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Comment"
          }
        },
        "files": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/File"
          }
        },
        "discussions": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Discussion"
          }
        },
        "documentVersions": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/DocumentVersion"
          }
        }
      }
    },
    "Document": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "templateId": {
          "type": ["string", "null"]
        },
        "user": {
          "$ref": "#/definitions/User"
        },
        "parentDocument": {
          "anyOf": [
            {
              "$ref": "#/definitions/Document"
            },
            {
              "type": "null"
            }
          ]
        },
        "children": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Document"
          }
        },
        "title": {
          "type": ["string", "null"]
        },
        "content": {
          "type": ["string", "null"]
        },
        "contentRich": {
          "type": ["number", "string", "boolean", "object", "array", "null"]
        },
        "yjsSnapshot": {
          "type": ["string", "null"]
        },
        "coverImage": {
          "type": ["string", "null"]
        },
        "icon": {
          "type": ["string", "null"]
        },
        "isPublished": {
          "type": "boolean",
          "default": false
        },
        "isArchived": {
          "type": "boolean",
          "default": false
        },
        "textStyle": {
          "type": "string",
          "default": "DEFAULT",
          "enum": ["DEFAULT", "SERIF", "MONO"]
        },
        "smallText": {
          "type": "boolean",
          "default": false
        },
        "fullWidth": {
          "type": "boolean",
          "default": false
        },
        "lockPage": {
          "type": "boolean",
          "default": false
        },
        "toc": {
          "type": "boolean",
          "default": true
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "updatedAt": {
          "type": "string",
          "format": "date-time"
        },
        "discussions": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Discussion"
          }
        },
        "documentVersions": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/DocumentVersion"
          }
        },
        "files": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/File"
          }
        }
      }
    },
    "DocumentVersion": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "document": {
          "$ref": "#/definitions/Document"
        },
        "user": {
          "$ref": "#/definitions/User"
        },
        "title": {
          "type": ["string", "null"]
        },
        "contentRich": {
          "type": ["number", "string", "boolean", "object", "array", "null"]
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "updatedAt": {
          "type": "string",
          "format": "date-time"
        }
      }
    },
    "Discussion": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "document": {
          "$ref": "#/definitions/Document"
        },
        "user": {
          "$ref": "#/definitions/User"
        },
        "documentContent": {
          "type": "string"
        },
        "documentContentRich": {
          "type": ["number", "string", "boolean", "object", "array", "null"]
        },
        "isResolved": {
          "type": "boolean",
          "default": false
        },
        "comments": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Comment"
          }
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "updatedAt": {
          "type": "string",
          "format": "date-time"
        }
      }
    },
    "Comment": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "user": {
          "$ref": "#/definitions/User"
        },
        "discussion": {
          "$ref": "#/definitions/Discussion"
        },
        "content": {
          "type": "string"
        },
        "contentRich": {
          "type": ["number", "string", "boolean", "object", "array", "null"]
        },
        "isEdited": {
          "type": "boolean",
          "default": false
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "updatedAt": {
          "type": "string",
          "format": "date-time"
        }
      }
    },
    "File": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "user": {
          "$ref": "#/definitions/User"
        },
        "document": {
          "anyOf": [
            {
              "$ref": "#/definitions/Document"
            },
            {
              "type": "null"
            }
          ]
        },
        "size": {
          "type": "integer"
        },
        "url": {
          "type": "string"
        },
        "appUrl": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "updatedAt": {
          "type": "string",
          "format": "date-time"
        }
      }
    }
  },
  "type": "object",
  "properties": {
    "session": {
      "$ref": "#/definitions/Session"
    },
    "oauthAccount": {
      "$ref": "#/definitions/OauthAccount"
    },
    "user": {
      "$ref": "#/definitions/User"
    },
    "document": {
      "$ref": "#/definitions/Document"
    },
    "documentVersion": {
      "$ref": "#/definitions/DocumentVersion"
    },
    "discussion": {
      "$ref": "#/definitions/Discussion"
    },
    "comment": {
      "$ref": "#/definitions/Comment"
    },
    "file": {
      "$ref": "#/definitions/File"
    }
  }
}
