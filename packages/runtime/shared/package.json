{
  "name": "@beep/runtime-shared",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "description": "The shared runtime package",
  "publishConfig": {
    "access": "restricted",
    "directory": "build"
  },
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json",
    "./*": "./src/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kriegcloud/beep-effect.git",
    "directory": "packages/runtime/shared"
  },
  "scripts": {
    "build": "pnpm build-esm && pnpm build-cjs && pnpm build-annotate",
    "dev": "tsc -b tsconfig.build.json --watch",
    "build-esm": "tsc -b tsconfig.build.json",
    "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
    "build-annotate": "babel build/esm --plugins annotate-pure-calls --out-dir build/esm --source-maps",
    "check": "tsc -b tsconfig.json",
    "test": "vitest run",
    "generate": "TSX_TSCONFIG_PATH=./tsconfig.build.json NODE_OPTIONS='--import tsx' drizzle-zero generate -c ./drizzle-zero.config.ts -o ./src/zero-schema.gen.ts",
    "clean": "rm -rf .tsbuildinfo node_modules coverage .next out dist build .turbo",
    "coverage": "vitest run --coverage",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "lint:circular": "pnpm madge -c ."
  },
  "dependencies": {
    "drizzle-zero": "*",
    "@rocicorp/zero": "*",
    "@effect-atom/atom-react": "*",
    "@beep/invariant": "workspace:^",
    "@beep/utils": "workspace:^",
    "@beep/schema": "workspace:^",
    "@beep/constants": "workspace:^",
    "@beep/errors": "workspace:^",
    "@beep/rules": "workspace:^",
    "@beep/logos": "workspace:^",
    "@beep/rete": "workspace:^",
    "@beep/shared-domain": "workspace:^",
    "@beep/shared-tables": "workspace:^",
    "@beep/core-env": "workspace:^",
    "@beep/ui": "workspace:^",
    "@beep/iam-domain": "workspace:^",
    "@beep/iam-tables": "workspace:^",
    "@beep/files-domain": "workspace:^",
    "@beep/files-tables": "workspace:^",
    "@opentelemetry/exporter-trace-otlp-http": "*",
    "@opentelemetry/sdk-trace-base": "*",
    "@opentelemetry/sdk-trace-node": "*",
    "@opentelemetry/sdk-trace-web": "*",
    "@opentelemetry/sdk-metrics": "*",
    "@opentelemetry/exporter-prometheus": "*",
    "@opentelemetry/exporter-trace-otlp-proto": "*",
    "@opentelemetry/exporter-logs-otlp-http": "*",
    "@opentelemetry/sdk-logs": "*",
    "@opentelemetry/context-async-hooks": "*",
    "@opentelemetry/exporter-metrics-otlp-proto": "*",
    "@opentelemetry/otlp-exporter-base": "*",
    "@opentelemetry/otlp-transformer": "*",
    "@opentelemetry/propagator-b3": "*",
    "@opentelemetry/resources": "*",
    "@opentelemetry/sdk-node": "*",
    "nuqs": "*",
    "@effect/platform": "*",
    "@effect/rpc": "*",
    "@effect/platform-browser": "*",
    "@tanstack/react-query": "*",
    "@tanstack/react-query-devtools": "*",
    "better-auth": "*",
    "effect": "*",
    "next": "*",
    "@effect/opentelemetry": "*",
    "@effect/experimental": "*",
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@types/react": "*",
    "@types/react-dom": "*",
    "@beep/types": "workspace:^",
    "@beep/testkit": "workspace:^",
    "@types/bun": "*"
  },
  "effect": {
    "generateExports": {
      "include": [
        "**/*.ts"
      ]
    },
    "generateIndex": {
      "include": [
        "**/*.ts"
      ]
    }
  }
}
