{
  "name": "@beep/runtime-server",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "description": "The server runtime package",
  "publishConfig": {
    "access": "restricted",
    "directory": "build"
  },
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json",
    "./*": "./src/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kriegcloud/beep-effect.git",
    "directory": "packages/runtime/server"
  },
  "scripts": {
    "build": "pnpm build-esm && pnpm build-cjs && pnpm build-annotate",
    "dev": "tsc -b tsconfig.build.json --watch",
    "build-esm": "tsc -b tsconfig.build.json",
    "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
    "build-annotate": "babel build/esm --plugins annotate-pure-calls --out-dir build/esm --source-maps",
    "check": "tsc -b tsconfig.json",
    "test": "vitest run",
    "clean": "rm -rf .tsbuildinfo node_modules coverage .next out dist build .turbo",
    "coverage": "vitest run --coverage",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "lint:circular": "pnpm madge -c .",
    "dotenvx": "dotenvx run -f ../../../.env --",
    "execute": "TSX_TSCONFIG_PATH=./tsconfig.src.json NODE_OPTIONS='--import tsx' pnpm dotenvx tsx ./src/execute.ts"
  },
  "peerDependencies": {
    "@effect/sql": "*",
    "@effect/sql-pg": "*",
    "@effect/sql-drizzle": "*",
    "@effect/experimental": "*",
    "@beep/invariant": "workspace:^",
    "@beep/utils": "workspace:^",
    "@beep/schema": "workspace:^",
    "@beep/constants": "workspace:^",
    "@beep/errors": "workspace:^",
    "@beep/rules": "workspace:^",
    "@beep/logos": "workspace:^",
    "@beep/rete": "workspace:^",
    "@beep/shared-domain": "workspace:^",
    "@beep/shared-tables": "workspace:^",
    "@beep/core-env": "workspace:^",
    "@beep/core-email": "workspace:^",
    "@beep/core-db": "workspace:^",
    "@beep/iam-domain": "workspace:^",
    "@beep/iam-tables": "workspace:^",
    "@beep/iam-infra": "workspace:^",
    "@beep/files-domain": "workspace:^",
    "@beep/files-tables": "workspace:^",
    "@beep/files-infra": "workspace:^",
    "@faker-js/faker": "^10.0.0",
    "postgres": "*",
    "@effect/platform": "*",
    "@effect/rpc": "*",
    "@effect/opentelemetry": "*",
    "@effect/platform-node": "*",
    "@effect/cluster": "*",
    "@effect/workflow": "*",
    "@opentelemetry/sdk-trace-node": "*",
    "resend": "*",
    "@opentelemetry/exporter-trace-otlp-http": "*",
    "drizzle-orm": "*",
    "next": "*",
    "effect": "*",
    "pg": "*",
    "react": "*",
    "react-dom": "*",
    "stripe": "*",
    "zod": "*",
    "@better-auth/stripe": "*",
    "better-call": "^1.0.18",
    "@dub/better-auth": "*",
    "@simplewebauthn/server": "*",
    "dub": "*",
    "jose": "*",
    "better-auth": "*",
    "uuid": "*"
  },
  "devDependencies": {
    "tsx": "*",
    "@types/pg": "*",
    "@beep/types": "workspace:^"
  },
  "effect": {
    "generateExports": {
      "include": [
        "**/*.ts"
      ]
    },
    "generateIndex": {
      "include": [
        "**/*.ts"
      ]
    }
  }
}
