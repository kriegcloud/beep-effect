{
  "$schema": "../../../node_modules/@effect/docgen/schema.json",
  "srcDir": "src",
  "outDir": "docs",
  "srcLink": "https://github.com/kriegcloud/beep-effect/tree/main/packages/runtime/server/src/",
  "exclude": ["src/internal/**/*.ts"],
  "examplesCompilerOptions": {
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "module": "ES2024",
    "target": "ES2024",
    "lib": ["ES2024", "DOM", "DOM.Iterable"],
    "paths": {
      "@beep/runtime-server": ["./src/index.ts"],
      "@beep/runtime-server/*": ["./src/*"],
      "@beep/shared-server": ["../../shared/server/src/index.js"],
      "@beep/shared-server/*": ["../../shared/server/src/*.js"],
      "@beep/shared-env": ["../../shared/env/src/index.js"],
      "@beep/shared-env/*": ["../../shared/env/src/*.js"],
      "@beep/schema": ["../../common/schema/src/index.js"],
      "@beep/schema/*": ["../../common/schema/src/*.js"],
      "@beep/constants": ["../../common/constants/src/index.js"],
      "@beep/constants/*": ["../../common/constants/src/*.js"],
      "@beep/errors/client": ["../../common/errors/src/client.js"],
      "@beep/errors/shared": ["../../common/errors/src/shared.js"],
      "@beep/errors/server": ["../../common/errors/src/server.js"],
      "@beep/shared-domain": ["../../shared/domain/src/index.js"],
      "@beep/shared-domain/*": ["../../shared/domain/src/*.js"],
      "@beep/iam-domain": ["../../iam/domain/src/index.js"],
      "@beep/iam-domain/*": ["../../iam/domain/src/*.js"],
      "@beep/iam-tables": ["../../iam/tables/src/index.js"],
      "@beep/iam-tables/*": ["../../iam/tables/src/*.js"],
      "@beep/iam-server": ["../../iam/server/src/index.js"],
      "@beep/iam-server/*": ["../../iam/server/src/*.js"],
      "@beep/documents-server": ["../../documents/server/src/index.js"],
      "@beep/documents-server/*": ["../../documents/server/src/*.js"],
      "@beep/customization-server": ["../../customization/server/src/index.js"],
      "@beep/customization-server/*": ["../../customization/server/src/*.js"]
    }
  }
}
