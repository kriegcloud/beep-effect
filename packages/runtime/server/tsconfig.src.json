{
  "extends": "../../../tsconfig.base.json",
  "include": ["src"],
  "references": [
    {
      "path": "../../common/types"
    },
    {
      "path": "../../common/invariant"
    },
    {
      "path": "../../common/utils"
    },
    {
      "path": "../../common/schema"
    },
    {
      "path": "../../common/constants"
    },
    {
      "path": "../../common/errors"
    },
    {
      "path": "../../common/rules"
    },
    {
      "path": "../../common/logos"
    },
    {
      "path": "../../common/rete"
    },
    {
      "path": "../../shared/domain"
    },
    {
      "path": "../../shared/tables"
    },
    {
      "path": "../../core/env"
    },
    {
      "path": "../../core/email"
    },
    {
      "path": "../../core/db"
    },
    {
      "path": "../../iam/infra"
    },
    {
      "path": "../../iam/domain"
    },
    {
      "path": "../../iam/tables"
    },
    {
      "path": "../../files/infra"
    },
    {
      "path": "../../files/domain"
    },
    {
      "path": "../../files/tables"
    }
  ],
  "compilerOptions": {
    "types": ["node", "bun"],
    "outDir": "build/src",
    "tsBuildInfoFile": ".tsbuildinfo/src.tsbuildinfo",
    "rootDir": "src",

    "moduleResolution": "bundler",
    "incremental": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "paths": {
      "*": ["./*"],
      "@beep/runtime-server/*": ["./src/*"],
      "@beep/types": ["../../common/types/src/index"],
      "@beep/types/*": ["../../common/types/src/*"],
      "@beep/invariant": ["../../common/invariant/src/index"],
      "@beep/invariant/*": ["../../common/invariant/src/*"],
      "@beep/utils": ["../../common/utils/src/index"],
      "@beep/utils/*": ["../../common/utils/src/*"],
      "@beep/schema": ["../../common/schema/src/index"],
      "@beep/schema/*": ["../../common/schema/src/*"],
      "@beep/constants": ["../../common/constants/src/index"],
      "@beep/constants/*": ["../../common/constants/src/*"],
      "@beep/errors/client": ["../../common/errors/src/client"],
      "@beep/errors/shared": ["../../common/errors/src/shared"],
      "@beep/errors/server": ["../../common/errors/src/server"],
      "@beep/rules": ["../../common/rules/src/index"],
      "@beep/rules/*": ["../../common/rules/src/*"],
      "@beep/logos": ["../../common/logos/src/index"],
      "@beep/logos/*": ["../../common/logos/src/*"],
      "@beep/rete": ["../../common/rete/src/index"],
      "@beep/rete/*": ["../../common/rete/src/*"],
      "@beep/shared-domain": ["../../shared/domain/src/index"],
      "@beep/shared-domain/*": ["../../shared/domain/src/*"],
      "@beep/shared-tables": ["../../shared/tables/src/index"],
      "@beep/shared-tables/*": ["../../shared/tables/src/*"],
      "@beep/core-env/client": ["../../core/env/src/client"],
      "@beep/core-env/server": ["../../core/env/src/server"],
      "@beep/core-email": ["../../core/email/src/index"],
      "@beep/core-email/*": ["../../core/email/src/*"],
      "@beep/core-db": ["../../core/db/src/index"],
      "@beep/core-db/*": ["../../core/db/src/*"],
      "@beep/iam-domain": ["../../iam/domain/src/index"],
      "@beep/iam-domain/*": ["../../iam/domain/src/*"],
      "@beep/iam-tables": ["../../iam/tables/src/index"],
      "@beep/iam-tables/*": ["../../iam/tables/src/*"],
      "@beep/iam-infra": ["../../iam/infra/src/index"],
      "@beep/iam-infra/*": ["../../iam/infra/src/*"],
      "@beep/files-domain": ["../../files/domain/src/index"],
      "@beep/files-domain/*": ["../../files/domain/src/*"],
      "@beep/files-tables": ["../../files/tables/src/index"],
      "@beep/files-tables/*": ["../../files/tables/src/*"],
      "@beep/files-infra": ["../../files/infra/src/index"],
      "@beep/files-infra/*": ["../../files/infra/src/*"]
    }
  }
}
