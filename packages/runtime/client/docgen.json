{
  "$schema": "../../../node_modules/@effect/docgen/schema.json",
  "srcDir": "src",
  "outDir": "docs",
  "srcLink": "https://github.com/kriegcloud/beep-effect/tree/main/packages/runtime/client/src/",
  "exclude": [
    "src/internal/**/*.ts"
  ],
  "examplesCompilerOptions": {
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "module": "ES2024",
    "target": "ES2024",
    "lib": [
      "ES2024",
      "DOM",
      "DOM.Iterable"
    ],
    "paths": {
      "@beep/runtime-client": [
        "./src/index.ts"
      ],
      "@beep/runtime-client/*": [
        "./src/*"
      ],
      "@beep/types": [
        "../../common/types/src/index.js"
      ],
      "@beep/types/*": [
        "../../common/types/src/*.js"
      ],
      "@beep/utils": [
        "../../common/utils/src/index.js"
      ],
      "@beep/utils/*": [
        "../../common/utils/src/*.js"
      ],
      "@beep/constants": [
        "../../common/constants/src/index.js"
      ],
      "@beep/constants/*": [
        "../../common/constants/src/*.js"
      ],
      "@beep/shared-env": [
        "../../shared/env/src/index.js"
      ],
      "@beep/shared-env/*": [
        "../../shared/env/src/*.js"
      ]
    }
  }
}