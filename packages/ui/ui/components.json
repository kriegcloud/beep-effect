    "assets": "@beep/ui/assets",
    "atoms": "@beep/ui/atoms",
    "branding": "@beep/ui/branding",
    "form": "@beep/ui/form",
    "icons": "@beep/ui/icons",
    "inputs": "@beep/ui/inputs",
    "layouts": "@beep/ui/layouts",
    "messages": "@beep/ui/messages",
    "molecules": "@beep/ui/molecules",
    "organisms": "@beep/ui/organisms",
    "progress": "@beep/ui/progress",
    "routing": "@beep/ui/routing",
    "sections": "@beep/ui/sections",
    "settings": "@beep/ui/settings",
    "theme": "@beep/ui/theme",
    "styled": "@beep/ui/styled"
