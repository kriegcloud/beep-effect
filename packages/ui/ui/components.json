{
  "$schema": "https://ui.shadcn.com/schema.json",
  "style": "base-nova",
  "rsc": true,
  "tsx": true,
  "tailwind": {
    "config": "",
    "css": "app/globals.css",
    "baseColor": "zinc",
    "cssVariables": true,
    "prefix": ""
  },
  "iconLibrary": "phosphor",
  "menuColor": "default",
  "menuAccent": "subtle",
  "aliases": {
    "utils": "@beep/ui-core/utils",
    "components": "@beep/ui/components",
    "hooks": "@beep/ui/hooks",
    "lib": "@beep/ui/lib",
    "ui": "@beep/ui/components",
