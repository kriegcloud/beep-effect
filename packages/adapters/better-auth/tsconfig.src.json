{
  "extends": "../../../tsconfig.base.json",
  "include": ["src"],
  "references": [
    {
      "path": "../../iam/domain"
    },
    {
      "path": "../../iam/tables"
    },
    {
      "path": "../../iam/db"
    },
    {
      "path": "../../env"
    },
    {
      "path": "../resend"
    },
    {
      "path": "../../persistence/db-scope"
    },
    {
      "path": "../../email"
    },
    {
      "path": "../../common/schema"
    },
    {
      "path": "../../common/invariant"
    },
    {
      "path": "../../common/utils"
    },
    {
      "path": "../../common/types"
    },
    {
      "path": "../../common/constants"
    },
    {
      "path": "../../shared/domain"
    },
    {
      "path": "../../shared/tables"
    },
        {
      "path": "../../runtime"
    }
  ],
  "compilerOptions": {
    "types": ["node"],
    "outDir": "build/src",
    "tsBuildInfoFile": ".tsbuildinfo/src.tsbuildinfo",
    "rootDir": "src",
    "baseUrl": ".",
    "jsx": "react-jsx",
    "module": "ESNext",
    "target": "ES2022",
    "lib": ["ES2024", "DOM", "DOM.Iterable"],
    "moduleResolution": "bundler",
    "incremental": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "paths": {
      "@beep/better-auth/*": ["./src/*"],
      "@beep/iam-domain": ["../../iam/domain/src/index"],
      "@beep/iam-domain/*": ["../../iam/domain/src/*"],
      "@beep/iam-tables": ["../../iam/tables/src/index"],
      "@beep/iam-tables/*": ["../../iam/tables/src/*"],
      "@beep/iam-db": ["../../iam/db/src/index"],
      "@beep/iam-db/*": ["../../iam/db/src/*"],
      "@beep/env/client": ["../../env/src/client"],
      "@beep/env/server": ["../../env/src/server"],
      "@beep/resend": ["../resend/src/index"],
      "@beep/resend/*": ["../resend/src/*"],
      "@beep/constants": ["../../common/constants/src/index"],
      "@beep/constants/*": ["../../common/constants/src/*"],
      "@beep/email": ["../../email/src/index"],
      "@beep/email/*": ["../../email/src/*"],
      "@beep/db-scope": ["../../persistence/db-scope/src/index"],
      "@beep/db-scope/*": ["../../persistence/db-scope/src/*"],
      "@beep/shared-domain": ["../../shared/domain/src/index"],
      "@beep/shared-domain/*": ["../../shared/domain/src/*"],
      "@beep/schema": ["../../common/schema/src/index"],
      "@beep/schema/*": ["../../common/schema/src/*"],
      "@beep/shared-tables": ["../../shared/tables/src/index"],
      "@beep/shared-tables/*": ["../../shared/tables/src/*"],
      "@beep/invariant": ["../../common/invariant/src/index"],
      "@beep/invariant/*": ["../../common/invariant/src/*"],
      "@beep/utils": ["../../common/utils/src/index"],
      "@beep/utils/*": ["../../common/utils/src/*"],
      "@beep/types": ["../../common/types/src/index"],
      "@beep/types/*": ["../../common/types/src/*"],
      "@beep/runtime/server": ["../../runtime/src/server"],
      "@beep/runtime/client": ["../../runtime/src/client"]
    }
  }
}
