{
  "$schema": "../../../node_modules/@effect/docgen/schema.json",
  "srcDir": "src",
  "outDir": "docs",
  "srcLink": "https://github.com/kriegcloud/beep-effect/tree/main/packages/iam/ui/src/",
  "exclude": ["src/internal/**/*.ts"],
  "examplesCompilerOptions": {
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "module": "ES2022",
    "target": "ES2022",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "paths": {
      "@beep/iam-ui": ["./src/index.ts"],
      "@beep/iam-ui/*": ["./src/*"],
      "@beep/iam-server": ["../server/src/index.js"],
      "@beep/iam-server/*": ["../server/src/*.js"],
      "@beep/identity": ["../../common/identity/src/index.js"],
      "@beep/identity/*": ["../../common/identity/src/*.js"],
      "@beep/types": ["../../common/types/src/index.js"],
      "@beep/types/*": ["../../common/types/src/*.js"],
      "@beep/shared-client": ["../../shared/client/src/index.js"],
      "@beep/shared-client/*": ["../../shared/client/src/*.js"],
      "@beep/shared-ui": ["../../shared/ui/src/index.js"],
      "@beep/shared-ui/*": ["../../shared/ui/src/*.js"],
      "@beep/schema": ["../../common/schema/src/index.js"],
      "@beep/schema/*": ["../../common/schema/src/*.js"],
      "@beep/invariant": ["../../common/invariant/src/index.js"],
      "@beep/invariant/*": ["../../common/invariant/src/*.js"],
      "@beep/utils": ["../../common/utils/src/index.js"],
      "@beep/utils/*": ["../../common/utils/src/*.js"],
      "@beep/iam-client": ["../client/src/index.js"],
      "@beep/iam-client/*": ["../client/src/*.js"],
      "@beep/iam-domain": ["../domain/src/index.js"],
      "@beep/iam-domain/*": ["../domain/src/*.js"],
      "@beep/constants": ["../../common/constants/src/index.js"],
      "@beep/constants/*": ["../../common/constants/src/*.js"],
      "@beep/mock": ["../../common/mock/src/index.js"],
      "@beep/mock/*": ["../../common/mock/src/*.js"],
      "@beep/errors/client": ["../../common/errors/src/client.js"],
      "@beep/errors/shared": ["../../common/errors/src/shared.js"],
      "@beep/errors/server": ["../../common/errors/src/server.js"],
      "@beep/contract": ["../../common/contract/src/index.js"],
      "@beep/contract/*": ["../../common/contract/src/*.js"],
      "@beep/runtime-client": ["../../runtime/client/src/index.js"],
      "@beep/runtime-client/*": ["../../runtime/client/src/*.js"],
      "@beep/shared-domain": ["../../shared/domain/src/index.js"],
      "@beep/shared-domain/*": ["../../shared/domain/src/*.js"]
    }
  }
}
