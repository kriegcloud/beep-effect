{
  "name": "@beep/iam-sdk",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "description": "The presentation layer for the IAM system",
  "publishConfig": {
    "access": "restricted",
    "directory": "build"
  },
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json",
    "./*": "./src/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kriegcloud/beep-effect.git",
    "directory": "packages/iam/sdk"
  },
  "scripts": {
    "build": "bun run build-esm && bun run build-cjs && bun run build-annotate",
    "dev": "tsc -b tsconfig.build.json --watch",
    "build-esm": "tsc -b tsconfig.build.json",
    "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
    "build-annotate": "babel build/esm --plugins annotate-pure-calls --out-dir build/esm --source-maps",
    "check": "tsc -b tsconfig.json",
    "test": "bun run dotenvx -- bunx bun test",
    "dotenvx": "dotenvx run -f ../../../.env --",
    "clean": "rm -rf .tsbuildinfo node_modules coverage .next out dist build .turbo",
    "coverage": "bun test --coverage",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "lint:circular": "bunx madge -c ."
  },
  "peerDependencies": {
    "mutative": "catalog:",
    "@beep/runtime-client": "workspace:^",
    "@effect-atom/atom-react": "catalog:",
    "@better-auth/stripe": "catalog:",
    "better-auth": "catalog:",
    "sonner": "catalog:",
    "dub": "catalog:",
    "stripe": "catalog:",
    "effect": "catalog:",
    "@dub/better-auth": "catalog:",
    "better-call": "catalog:",
    "jose": "catalog:",
    "@beep/utils": "workspace:^",
    "@beep/invariant": "workspace:^",
    "next": "catalog:",
    "@beep/schema": "workspace:^",
    "@beep/errors": "workspace:^",
    "@beep/constants": "workspace:^",
    "@beep/ui": "workspace:^",
    "@beep/ui-core": "workspace:^",
    "@beep/core-env": "workspace:^",
    "@beep/shared-domain": "workspace:^",
    "@beep/iam-domain": "workspace:^"
  },
  "devDependencies": {
    "mutative": "catalog:",
    "@beep/runtime-client": "workspace:^",
    "@effect-atom/atom-react": "catalog:",
    "@better-auth/stripe": "catalog:",
    "better-auth": "catalog:",
    "sonner": "catalog:",
    "dub": "catalog:",
    "stripe": "catalog:",
    "effect": "catalog:",
    "@dub/better-auth": "catalog:",
    "better-call": "catalog:",
    "jose": "catalog:",
    "@beep/utils": "workspace:^",
    "@beep/invariant": "workspace:^",
    "next": "catalog:",
    "@beep/schema": "workspace:^",
    "@beep/errors": "workspace:^",
    "@beep/constants": "workspace:^",
    "@beep/ui": "workspace:^",
    "@beep/ui-core": "workspace:^",
    "@beep/core-env": "workspace:^",
    "@beep/shared-domain": "workspace:^",
    "@beep/iam-domain": "workspace:^",
    "@types/bun": "catalog:",
    "@beep/types": "workspace:^"
  },
  "effect": {
    "generateExports": {
      "include": [
        "**/*.ts"
      ]
    },
    "generateIndex": {
      "include": [
        "**/*.ts"
      ]
    }
  }
}
