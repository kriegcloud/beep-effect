{
  "$schema": "../../../node_modules/@effect/docgen/schema.json",
  "srcDir": "src",
  "outDir": "docs",
  "srcLink": "https://github.com/kriegcloud/beep-effect/tree/main/packages/iam/tables/src/",
  "exclude": [
    "src/internal/**/*.ts"
  ],
  "examplesCompilerOptions": {
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "module": "ES2022",
    "target": "ES2022",
    "lib": [
      "ES2022",
      "DOM",
      "DOM.Iterable"
    ],
    "paths": {
      "@beep/iam-tables": [
        "./src/index.ts"
      ],
      "@beep/iam-tables/*": [
        "./src/*"
      ],
      "@beep/types": [
        "../../common/types/src/index.js"
      ],
      "@beep/types/*": [
        "../../common/types/src/*.js"
      ],
      "@beep/identity": [
        "../../common/identity/src/index.js"
      ],
      "@beep/identity/*": [
        "../../common/identity/src/*.js"
      ],
      "@beep/shared-tables": [
        "../../shared/tables/src/index.js"
      ],
      "@beep/shared-tables/*": [
        "../../shared/tables/src/*.js"
      ],
      "@beep/schema": [
        "../../common/schema/src/index.js"
      ],
      "@beep/schema/*": [
        "../../common/schema/src/*.js"
      ],
      "@beep/utils": [
        "../../common/utils/src/index.js"
      ],
      "@beep/utils/*": [
        "../../common/utils/src/*.js"
      ],
      "@beep/invariant": [
        "../../common/invariant/src/index.js"
      ],
      "@beep/invariant/*": [
        "../../common/invariant/src/*.js"
      ],
      "@beep/shared-domain": [
        "../../shared/domain/src/index.js"
      ],
      "@beep/shared-domain/*": [
        "../../shared/domain/src/*.js"
      ],
      "@beep/iam-domain": [
        "../domain/src/index.js"
      ],
      "@beep/iam-domain/*": [
        "../domain/src/*.js"
      ]
    }
  }
}