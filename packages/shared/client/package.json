{
  "name": "@beep/shared-client",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "description": "A package for shared client logic, services and more",
  "publishConfig": {
    "access": "restricted",
    "directory": "build"
  },
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json",
    "./*": "./src/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kriegcloud/beep-effect.git",
    "directory": "packages/shared/client"
  },
  "scripts": {
    "dev": "pnpm dotenvx tsc -b tsconfig.build.json --watch",
    "build": "pnpm build-esm && pnpm build-cjs && pnpm build-annotate",
    "build-esm": "tsc -b tsconfig.build.json",
    "build-cjs": "babel build/esm --plugins babel-plugin-transform-next-use-client --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --presets @babel/preset-react --out-dir build/cjs --source-maps ",
    "build-annotate": "babel build/esm --plugins babel-plugin-transform-next-use-client --plugins annotate-pure-calls --presets @babel/preset-react --out-dir build/esm --source-maps",
    "check": "tsc -b tsconfig.json",
    "test": "vitest run",
    "clean": "rm -rf .tsbuildinfo node_modules coverage .next out dist build .turbo",
    "coverage": "vitest run --coverage",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "lint:circular": "pnpm madge -c ."
  },
  "dependencies": {
    "@beep/invariant": "workspace:^",
    "@beep/schema": "workspace:^",
    "@beep/ui": "workspace:^",
    "@beep/utils": "workspace:^",
    "@beep/logos": "workspace:^",
    "@beep/rete": "workspace:^",
    "@beep/env": "workspace:^",
    "@effect/cluster": "*",
    "@effect/workflow": "*",
    "@emotion/cache": "*",
    "@emotion/react": "*",
    "@emotion/styled": "*",
    "@fontsource-variable/dm-sans": "*",
    "@fontsource-variable/inter": "*",
    "@fontsource-variable/nunito-sans": "*",
    "@fontsource-variable/public-sans": "*",
    "@fontsource/barlow": "*",
    "@jsonforms/core": "*",
    "@jsonforms/material-renderers": "*",
    "@jsonforms/react": "*",
    "@mui/lab": "*",
    "@mui/material": "*",
    "@mui/x-date-pickers": "*",
    "@statelyai/inspect": "*",
    "@xstate/react": "*",
    "ajv": "*",
    "ajv-formats": "*",
    "dayjs": "*",
    "effect": "*",
    "json-logic-js": "*",
    "react": "*",
    "react-dom": "*",
    "xstate": "*",
    "@emotion/server": "*",
    "@iconify/react": "*",
    "@mui/icons-material": "*",
    "@mui/system": "*",
    "@mui/stylis-plugin-rtl": "*",
    "@mui/x-data-grid": "*",
    "@mui/x-tree-view": "*",
    "@tanstack/react-form": "*",
    "@tiptap/core": "*",
    "@tiptap/extension-code-block": "*",
    "@tiptap/extension-code-block-lowlight": "*",
    "@tiptap/extension-heading": "*",
    "@tiptap/extension-image": "*",
    "@tiptap/extension-link": "*",
    "@tiptap/extension-placeholder": "*",
    "@tiptap/extension-text-align": "*",
    "@tiptap/extension-underline": "*",
    "@tiptap/pm": "*",
    "@tiptap/react": "*",
    "@tiptap/starter-kit": "*",
