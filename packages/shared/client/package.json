    "babel-plugin-transform-next-use-client": "*",
    "@babel/preset-react": "*"
  },
  "effect": {
    "generateExports": {
      "include": [
        "**/*.ts"
      ]
    },
    "generateIndex": {
      "include": [
        "**/*.ts"
      ]
    }
  }
}
