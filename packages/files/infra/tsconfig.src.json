{
  "extends": "../../../tsconfig.base.json",
  "include": ["src"],
  "references": [
    {
      "path": "../../persistence/db-scope"
    },
    {
      "path": "../../env"
    },
    {
      "path": "../../shared/tables"
    },
    {
      "path": "../../common/schema"
    },
    {
      "path": "../../common/utils"
    },
    {
      "path": "../../common/invariant"
    },
    {
      "path": "../../common/types"
    },
    {
      "path": "../application"
    },
    {
      "path": "../domain"
    }
  ],
  "compilerOptions": {
    "types": ["node"],
    "outDir": "build/src",
    "tsBuildInfoFile": ".tsbuildinfo/src.tsbuildinfo",
    "rootDir": "src",
    "baseUrl": ".",
    "moduleResolution": "bundler",
    "incremental": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "paths": {
      "@beep/files-db/*": ["./src/*"],
      "@beep/db-scope": ["../../persistence/db-scope/src/index"],
      "@beep/db-scope/*": ["../../persistence/db-scope/src/*"],
      "@beep/env/client": ["../../env/src/client"],
      "@beep/env/server": ["../../env/src/server"],
      "@beep/shared-tables": ["../../shared/tables/src/index"],
      "@beep/shared-tables/*": ["../../shared/tables/src/*"],
      "@beep/schema": ["../../common/schema/src/index"],
      "@beep/schema/*": ["../../common/schema/src/*"],
      "@beep/invariant": ["../../common/invariant/src/index"],
      "@beep/invariant/*": ["../../common/invariant/src/*"],
      "@beep/utils": ["../../common/utils/src/index"],
      "@beep/utils/*": ["../../common/utils/src/*"],
      "@beep/types": ["../../common/types/src/index"],
      "@beep/types/*": ["../../common/types/src/*"],
      "@beep/files-application": ["../application/src/index"],
      "@beep/files-domain": ["../domain/src/index"]
    }
  }
}
