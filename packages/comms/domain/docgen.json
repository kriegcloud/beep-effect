{
  "$schema": "../../../node_modules/@effect/docgen/schema.json",
  "srcDir": "src",
  "outDir": "docs",
  "srcLink": "https://github.com/kriegcloud/beep-effect/tree/main/packages/comms/domain/src/",
  "exclude": [
    "src/internal/**/*.ts"
  ],
  "examplesCompilerOptions": {
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "module": "ES2024",
    "target": "ES2024",
    "lib": [
      "ES2024",
      "DOM",
      "DOM.Iterable"
    ],
    "paths": {
      "@beep/comms-domain": [
        "./src/index.ts"
      ],
      "@beep/comms-domain/*": [
        "./src/*"
      ],
      "@beep/shared-domain": [
        "../../shared/domain/src/index.js"
      ],
      "@beep/shared-domain/*": [
        "../../shared/domain/src/*.js"
      ],
      "@beep/schema": [
        "../../common/schema/src/index.js"
      ],
      "@beep/schema/*": [
        "../../common/schema/src/*.js"
      ]
    }
  }
}