{
  "$schema": "../../../node_modules/@effect/docgen/schema.json",
  "srcDir": "src",
  "outDir": "docs",
  "srcLink": "https://github.com/kriegcloud/beep-effect/tree/main/packages/common/utils/src/",
  "exclude": [
    "src/internal/**/*.ts",
    "src/experimental/contract/**/*.ts",
    "src/**/_id.ts"
  ],
  "parseCompilerOptions": {
    "baseUrl": ".",
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "module": "ES2023",
    "target": "ES2023",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "allowImportingTsExtensions": true,
    "rewriteRelativeImportExtensions": true,
    "paths": {
      "@beep/utils": ["./src/index.ts"],
      "@beep/utils/*": ["./src/*"],
      "@beep/types": ["../types/src/index.ts"],
      "@beep/types/*": ["../types/src/*"],
      "@beep/invariant": ["../invariant/src/index.ts"],
      "@beep/invariant/*": ["../invariant/src/*"]
    }
  },
  "examplesCompilerOptions": {
    "baseUrl": ".",
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "module": "ES2023",
    "target": "ES2023",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "allowImportingTsExtensions": true,
    "rewriteRelativeImportExtensions": true,
    "paths": {
      "@beep/utils": ["./src/index.ts"],
      "@beep/utils/*": ["./src/*"],
      "@beep/types": ["../types/src/index.ts"],
      "@beep/types/*": ["../types/src/*"],
      "@beep/invariant": ["../invariant/src/index.ts"],
      "@beep/invariant/*": ["../invariant/src/*"]
    }
  }
}
