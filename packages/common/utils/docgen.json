{
  "$schema": "../../../node_modules/@effect/docgen/schema.json",
  "srcDir": "src",
  "outDir": "docs",
  "srcLink": "https://github.com/kriegcloud/beep-effect/tree/main/packages/common/utils/src/",
  "exclude": ["src/internal/**/*.ts"],
  "examplesCompilerOptions": {
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "module": "ES2024",
    "target": "ES2024",
    "lib": ["ES2024", "DOM", "DOM.Iterable"],
    "paths": {
      "@beep/utils": ["./src/index.ts"],
      "@beep/utils/*": ["./src/*"],
      "@beep/testkit": ["../../../tooling/testkit/src/index.js"],
      "@beep/testkit/*": ["../../../tooling/testkit/src/*.js"],
      "@beep/types": ["../types/src/index.js"],
      "@beep/types/*": ["../types/src/*.js"],
      "@beep/invariant": ["../invariant/src/index.js"],
      "@beep/invariant/*": ["../invariant/src/*.js"]
    }
  }
}
