{
  "$schema": "../../../node_modules/@effect/docgen/schema.json",
  "srcDir": "src",
  "outDir": "docs",
  "srcLink": "https://github.com/kriegcloud/beep-effect/tree/main/packages/common/constants/src/",
  "exclude": [
    "src/internal/**/*.ts"
  ],
  "examplesCompilerOptions": {
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "module": "ES2022",
    "target": "ES2022",
    "lib": [
      "ES2022",
      "DOM",
      "DOM.Iterable"
    ],
    "paths": {
      "@beep/constants": [
        "./src/index.ts"
      ],
      "@beep/constants/*": [
        "./src/*"
      ],
      "@beep/schema": [
        "../schema/src/index.js"
      ],
      "@beep/schema/*": [
        "../schema/src/*.js"
      ],
      "@beep/utils": [
        "../utils/src/index.js"
      ],
      "@beep/utils/*": [
        "../utils/src/*.js"
      ],
      "@beep/invariant": [
        "../invariant/src/index.js"
      ],
      "@beep/invariant/*": [
        "../invariant/src/*.js"
      ],
      "@beep/identity": [
        "../identity/src/index.js"
      ],
      "@beep/identity/*": [
        "../identity/src/*.js"
      ],
      "@beep/types": [
        "../types/src/index.js"
      ],
      "@beep/types/*": [
        "../types/src/*.js"
      ]
    }
  }
}