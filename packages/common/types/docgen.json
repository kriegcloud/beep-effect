{
  "$schema": "../../../node_modules/@effect/docgen/schema.json",
  "srcDir": "src",
  "outDir": "docs",
  "srcLink": "https://github.com/kriegcloud/beep-effect/tree/main/packages/common/types/src/",
  "exclude": ["src/index.ts"],
  "parseCompilerOptions": {
    "baseUrl": ".",
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "module": "ES2023",
    "target": "ES2023",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "allowImportingTsExtensions": true,
    "rewriteRelativeImportExtensions": true,
    "paths": {
      "@beep/types": ["./src/index.ts"],
      "@beep/types/*": ["./src/*"],
      "@beep/utils": ["../utils/src/index.ts"],
      "@beep/utils/*": ["../utils/src/*"]
    }
  },
  "examplesCompilerOptions": {
    "baseUrl": ".",
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "module": "ES2023",
    "target": "ES2023",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "allowImportingTsExtensions": true,
    "rewriteRelativeImportExtensions": true,
    "paths": {
      "@beep/types": ["./src/index.ts"],
      "@beep/types/*": ["./src/*"],
      "@beep/utils": ["../utils/src/index.ts"],
      "@beep/utils/*": ["../utils/src/*"]
    }
  }
}
