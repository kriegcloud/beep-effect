{
  "name": "@beep/types",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "description": "A library for common types",
  "publishConfig": {
    "access": "restricted",
    "directory": "build"
  },
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js",
      "default": "./src/index.js"
    },
    "./package.json": "./package.json",
    "./*": {
      "types": "./src/*.d.ts",
      "import": "./src/*.js",
      "default": "./src/*.js"
    }
  },
  "types": "./src/index.d.ts",
  "main": "./src/index.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:kriegcloud/beep-effect.git",
    "directory": "packages/common/types"
  },
  "scripts": {
    "build": "bun run build-esm && bun run build-cjs && bun run build-annotate",
    "dev": "tsgo -b tsconfig.build.json --watch",
    "build-esm": "tsgo -b tsconfig.build.json",
    "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
    "build-annotate": "babel build/esm --plugins annotate-pure-calls --out-dir build/esm --source-maps",
    "check": "tsgo -b tsconfig.json",
    "test": "bun test",
    "clean": "rm -rf .tsbuildinfo node_modules coverage .next out dist build .turbo",
    "coverage": "bun test --coverage",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "lint:circular": "bunx madge -c ."
  },
  "peerDependencies": {
    "effect": "catalog:"
  },
  "devDependencies": {
    "string-ts": "catalog:",
    "@beep/testkit": "workspace:^",
    "effect": "catalog:",
    "@types/bun": "catalog:"
  },
  "effect": {
    "generateExports": {
      "include": [
        "**/*.ts"
      ]
    },
    "generateIndex": {
      "include": [
        "**/*.ts"
      ]
    }
  }
}
