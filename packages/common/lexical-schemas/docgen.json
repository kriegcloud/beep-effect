{
  "$schema": "../../../node_modules/@effect/docgen/schema.json",
  "srcDir": "src",
  "outDir": "docs",
  "srcLink": "https://github.com/kriegcloud/beep-effect/tree/main/packages/common/lexical-schemas/src/",
  "exclude": ["src/internal/**/*.ts"],
  "examplesCompilerOptions": {
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "module": "ES2024",
    "target": "ES2024",
    "lib": ["ES2024", "DOM", "DOM.Iterable"],
    "paths": {
      "@beep/lexical-schemas": ["./src/index.ts"],
      "@beep/lexical-schemas/*": ["./src/*"],
      "@beep/schema": ["../schema/src/index.js"],
      "@beep/schema/*": ["../schema/src/*.js"],
      "@beep/identity": ["../identity/src/index.js"],
      "@beep/identity/*": ["../identity/src/*.js"]
    }
  }
}
