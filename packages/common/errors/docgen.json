{
  "$schema": "../../../node_modules/@effect/docgen/schema.json",
  "srcDir": "src",
  "outDir": "docs",
  "srcLink": "https://github.com/kriegcloud/beep-effect/tree/main/packages/common/errors/src/",
  "exclude": [],
  "parseCompilerOptions": {
    "baseUrl": ".",
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "target": "ES2023",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "allowImportingTsExtensions": true,
    "rewriteRelativeImportExtensions": true,
    "paths": {
      "@beep/errors": ["./src/index.ts"],
      "@beep/errors/*": ["./src/*"],
      "@beep/errors/client": ["./src/client.ts"],
      "@beep/errors/server": ["./src/server.ts"],
      "@beep/errors/shared": ["./src/shared.ts"],
      "@beep/errors/errors": ["./src/errors.ts"],
      "@beep/constants": ["../constants/src/index.ts"],
      "@beep/constants/*": ["../constants/src/*"],
      "@beep/schema": ["../schema/src/index.ts"],
      "@beep/schema/*": ["../schema/src/*"],
      "@beep/utils": ["../utils/src/index.ts"],
      "@beep/utils/*": ["../utils/src/*"],
      "@beep/invariant": ["../invariant/src/index.ts"],
      "@beep/invariant/*": ["../invariant/src/*"],
      "@beep/identity": ["../identity/src/index.ts"],
      "@beep/identity/*": ["../identity/src/*"],
      "@beep/types": ["../types/src/index.ts"],
      "@beep/types/*": ["../types/src/*"]
    }
  },
  "examplesCompilerOptions": {
    "baseUrl": ".",
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "target": "ES2023",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "allowImportingTsExtensions": true,
    "rewriteRelativeImportExtensions": true,
    "paths": {
      "@beep/errors": ["./src/index.ts"],
      "@beep/errors/*": ["./src/*"],
      "@beep/errors/client": ["./src/client.ts"],
      "@beep/errors/server": ["./src/server.ts"],
      "@beep/errors/shared": ["./src/shared.ts"],
      "@beep/errors/errors": ["./src/errors.ts"],
      "@beep/constants": ["../constants/src/index.ts"],
      "@beep/constants/*": ["../constants/src/*"],
      "@beep/schema": ["../schema/src/index.ts"],
      "@beep/schema/*": ["../schema/src/*"],
      "@beep/utils": ["../utils/src/index.ts"],
      "@beep/utils/*": ["../utils/src/*"],
      "@beep/invariant": ["../invariant/src/index.ts"],
      "@beep/invariant/*": ["../invariant/src/*"],
      "@beep/identity": ["../identity/src/index.ts"],
      "@beep/identity/*": ["../identity/src/*"],
      "@beep/types": ["../types/src/index.ts"],
      "@beep/types/*": ["../types/src/*"]
    }
  }
}
