{
  "name": "@beep/theme",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "license": "MIT",
  "description": "The shared ui theme for beep-effect which combines @mui/material",
  "repository": {
    "type": "git",
    "url": "git@github.com:kriegcloud/beep-effect.git",
    "directory": "packages/theme"
  },
  "scripts": {
    "codegen": "build-utils prepare-v2",
    "dotenvx": "dotenvx run -f ../../.env --",
    "dev": "pnpm dotenvx tsc -b tsconfig.build.json --watch",
    "build": "pnpm build-esm && pnpm build-cjs && pnpm build-annotate",
    "build-esm": "tsc -b tsconfig.build.json",
    "build-cjs": "babel build/esm --plugins babel-plugin-transform-next-use-client --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --presets @babel/preset-react --out-dir build/cjs --source-maps ",
    "build-annotate": "babel build/esm --plugins babel-plugin-transform-next-use-client --plugins annotate-pure-calls --presets @babel/preset-react --out-dir build/esm --source-maps",
    "check": "tsc -b tsconfig.json",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "clean": "rm -rf .tsbuildinfo node_modules coverage .next out dist build .turbo",
    "lint:circular": "pnpm madge -c .",
    "execute": "pnpm tsx --tsconfig ./tsconfig.src.json ./src/execute.ts"
  },
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json",
    "./*": "./src/*.ts"
  },
  "peerDependencies": {
    "@beep/invariant": "workspace:^",
    "@beep/schema": "workspace:^",
    "@beep/utils": "workspace:^",
    "@beep/constants": "workspace:^",
    "@beep/shared-domain": "workspace:^",
    "@mui/material": "*",
    "next": "*",
    "@mui/lab": "*",
    "@mui/x-data-grid": "*",
    "@mui/x-date-pickers": "*",
    "@mui/x-tree-view": "*",
    "stylis-plugin-rtl": "*",
    "@emotion/cache": "*",
    "@emotion/react": "*",
    "effect": "*",
    "@iconify/react": "*",
    "@mui/system": "*",
    "react": "*",
    "@mui/stylis-plugin-rtl": "*",
    "lowlight": "*",
    "stylis": "*",
    "react-dom": "*"
  },
  "peerDependenciesMeta": {
    "@mui/icons-material": {
      "optional": true
    }
  },
  "devDependencies": {
    "@beep/types": "workspace:^",
    "@types/react": "*",
    "@types/react-dom": "*",
    "babel-plugin-transform-next-use-client": "*",
    "copyfiles": "*",
    "@babel/preset-react": "*"
  }
}
