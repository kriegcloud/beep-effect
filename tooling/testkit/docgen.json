{
  "$schema": "../../node_modules/@effect/docgen/schema.json",
  "srcDir": "src",
  "outDir": "docs",
  "srcLink": "https://github.com/kriegcloud/beep-effect/tree/main/tooling/testkit/src/",
  "exclude": ["src/internal/**/*.ts"],
  "examplesCompilerOptions": {
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "module": "ES2024",
    "target": "ES2024",
    "lib": ["ES2024", "DOM", "DOM.Iterable"],
    "paths": {
      "@beep/testkit": ["./src/index.ts"],
      "@beep/testkit/*": ["./src/*"]
    }
  }
}
