{
  "$schema": "../../node_modules/@effect/docgen/schema.json",
  "srcDir": "src",
  "outDir": "docs",
  "srcLink": "https://github.com/kriegcloud/beep-effect/tree/main/tooling/utils/src/",
  "exclude": ["src/internal/**/*.ts"],
  "parseCompilerOptions": {
    "baseUrl": ".",
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "target": "ES2024",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "allowImportingTsExtensions": false,
    "rewriteRelativeImportExtensions": true,
    "paths": {
      "@beep/tooling-utils": ["./src/index.ts"],
      "@beep/tooling-utils/*": ["./src/*.ts"],
      "@beep/types": ["../../packages/common/types/src/index.ts"],
      "@beep/types/*": ["../../packages/common/types/src/*.ts"]
    }
  },
  "examplesCompilerOptions": {
    "baseUrl": ".",
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "target": "ES2024",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "allowImportingTsExtensions": false,
    "rewriteRelativeImportExtensions": true,
    "paths": {
      "@beep/tooling-utils": ["./src/index.ts"],
      "@beep/tooling-utils/*": ["./src/*.ts"],
      "@beep/types": ["../../packages/common/types/src/index.ts"],
      "@beep/types/*": ["../../packages/common/types/src/*.ts"]
    }
  }
}
