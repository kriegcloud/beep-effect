{
  "extends": "../../tsconfig.base.json",
  "include": ["src"],
  "exclude": [],
  "references": [
    {
      "path": "../../packages/common/types"
    }
  ],
  "compilerOptions": {
    "types": ["bun", "node"],
    "outDir": "build/src",
    "tsBuildInfoFile": ".tsbuildinfo/src.tsbuildinfo",
    "rootDir": "src",
    "module": "ESNext",
    "isolatedModules": false,
    "moduleResolution": "bundler",
    "incremental": true,
    "skipLibCheck": true,
    "esModuleInterop": true
  }
}
