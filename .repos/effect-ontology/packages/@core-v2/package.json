{
    "name": "@effect-ontology/core-v2",
    "version": "0.0.0",
    "type": "module",
    "private": true,
    "description": "Effect-native knowledge extraction core - v2 migration",
    "bin": {
        "effect-onto": "./src/cli.ts"
    },
    "exports": {
        ".": "./src/index.ts",
        "./Domain": "./src/Domain/index.ts",
        "./Service": "./src/Service/index.ts",
        "./Workflow": "./src/Workflow/index.ts",
        "./Schema": "./src/Schema/index.ts",
        "./Runtime": "./src/Runtime/index.ts"
    },
    "scripts": {
        "build": "tsc --skipLibCheck --project tsconfig.build.json",
        "serve": "bun run src/server.ts",
        "serve:postgres": "bun --env-file=.env.postgres run src/server.ts",
        "test": "vitest --run",
        "test:watch": "vitest",
        "test:ui": "vitest --ui",
        "test:e2e": "vitest --run test/E2E",
        "test:e2e:watch": "vitest test/E2E",
        "test:e2e:real": "E2E_TEST_MODE=cheap vitest --run test/E2E",
        "check": "tsc -b tsconfig.json",
        "db:start": "docker compose -f ../../docker-compose.yml up -d postgres",
        "db:stop": "docker compose -f ../../docker-compose.yml down postgres",
        "db:logs": "docker compose -f ../../docker-compose.yml logs -f postgres",
        "db:psql": "docker exec -it workflow-postgres psql -U workflow -d workflow",
        "db:tables": "docker exec -it workflow-postgres psql -U workflow -d workflow -c '\\dt workflow_*'",
        "extract": "bun run src/cli.ts extract"
    },
    "dependencies": {
        "@effect/ai": "^0.32.1",
        "@effect/ai-anthropic": "^0.22.0",
        "@effect/ai-google": "^0.11.0",
        "@effect/ai-openai": "^0.35.0",
        "@effect/cli": "^0.58.0",
        "@effect/cluster": "^0.55.0",
        "@effect/experimental": "0.58.0",
        "@effect/opentelemetry": "^0.59.1",
        "@effect/platform": "0.94.0",
        "@effect/platform-bun": "^0.87.0",
        "@effect/printer": "^0.47.0",
        "@effect/rpc": "^0.72.2",
        "@effect/sql": "^0.48.6",
        "@effect/sql-drizzle": "^0.47.0",
        "@effect/sql-pg": "^0.49.7",
        "@effect/typeclass": "^0.38.0",
        "@effect/workflow": "^0.15.2",
        "@google-cloud/pubsub": "^5.2.0",
        "@google-cloud/storage": "^7.18.0",
        "@opentelemetry/api": "^1.9.0",
        "@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
        "@opentelemetry/sdk-logs": "^0.208.0",
        "@opentelemetry/sdk-metrics": "^2.2.0",
        "@opentelemetry/sdk-trace-base": "^2.2.0",
        "@opentelemetry/sdk-trace-node": "^2.2.0",
        "@opentelemetry/sdk-trace-web": "^2.2.0",
        "@xenova/transformers": "^2.17.2",
        "drizzle-orm": "^0.45.1",
        "effect": "^3.19.13",
        "n3": "^1.26.0",
        "oxigraph": "^0.5.2",
        "shacl-engine": "^1.1.0",
        "uuid": "^13.0.0",
        "wink-bm25-text-search": "^3.1.2",
        "wink-eng-lite-web-model": "^1.8.1",
        "wink-nlp": "^2.4.0",
        "wink-nlp-utils": "^2.1.0"
    },
    "devDependencies": {
        "@effect/vitest": "^0.25.1",
        "@fast-check/vitest": "^0.2.3",
        "@rdfjs/types": "^2.0.1",
        "@types/n3": "^1.26.1",
        "@types/node": "^22.5.2",
        "@types/uuid": "^11.0.0",
        "fast-check": "^4.3.0",
        "typescript": "^5.6.2",
        "vitest": "^3.2.0"
    }
}
