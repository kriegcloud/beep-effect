{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "globalDependencies": [
    "**/.env",
    "**/.env.*",
    "tsconfig.json",
    "tsconfig.build.json",
    "tsconfig.base.json"
  ],
  "globalEnv": [
    "TURBO_TOKEN",
    "TURBO_TEAM"
  ],
  "globalPassThroughEnv": [
    "NODE_TLS_REJECT_UNAUTHORIZED",
    "AWS_*",
    "SST_*"
  ],
  "tasks": {
    "build": {
      "persistent": false,
      "cache": true,
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        "dist/**",
        "build/**",
        ".next/**",
        "next-env.d.ts",
        ".expo/**",
        "_next/**",
        ".output/**",
        ".vercel/output/**",
        "**/.tsbuildinfo/**",
        "!**/.turbo/**",
        "!.next/cache/**"
      ]
    },
    "web#build": {
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "dependsOn": [
        "^build"
      ],
      "env": [
        "NEXT_PUBLIC_STATIC_URL"
      ],
      "outputs": [
        "dist/**",
        "build/**",
        ".next/**",
        "!.next/cache/**",
        "_next/**",
        "next-env.d.ts",
        ".expo/**",
        ".output/**",
        ".vercel/output/**",
        "**/.tsbuildinfo/**",
        "!**/.turbo/**"
      ]
    },
    "db:generate": {
      "dependsOn": [
        "^db:generate"
      ],
      "inputs": [
        "$TURBO_DEFAULT$"
      ]
    },
    "db:push": {
      "dependsOn": [
        "^db:push"
      ],
      "inputs": [
        "$TURBO_DEFAULT$"
      ]
    },
    "db:migrate": {
      "dependsOn": [
        "^db:migrate"
      ],
      "inputs": [
        "$TURBO_DEFAULT$"
      ]
    },
    "db:studio": {
      "dependsOn": [
        "^db:studio"
      ],
      "inputs": [
        "$TURBO_DEFAULT$"
      ]
    },
    "bootstrap": {
      "cache": false,
      "persistent": false
    },
    "lint": {
      "cache": true,
      "dependsOn": [
        "^lint"
      ],
      "inputs": [
        "$TURBO_DEFAULT$"
      ]
    },
    "lint:fix": {
      "cache": true,
      "dependsOn": [
        "^lint:fix"
      ],
      "inputs": [
        "$TURBO_DEFAULT$"
      ]
    },
    "lint:circular": {
      "cache": true,
      "dependsOn": [
        "^lint:circular"
      ],
      "inputs": [
        "$TURBO_DEFAULT$"
      ]
    },
    "check": {
      "dependsOn": [
        "^check"
      ],
      "inputs": [
        "$TURBO_DEFAULT$"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:https": {
      "cache": false,
      "persistent": true
    },
    "build:turbo": {
      "cache": true,
      "persistent": true
    },
    "gen:beep-paths": {
      "cache": false,
      "persistent": false
    },
    "iconify": {
      "cache": false,
      "persistent": false,
      "dependsOn": [
        "^build"
      ]
    },
    "start": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$"
      ]
    },
    "coverage": {
      "dependsOn": [
        "^test"
      ]
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    }
  }
}
