{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "globalDependencies": [
    "**/.env",
    "**/.env.*",
    "tsconfig.json",
    "tsconfig.build.json",
    "tsconfig.base.jsonc"
  ],
  "globalEnv": [
    "TURBO_TOKEN",
    "TURBO_TEAM"
  ],
  "globalPassThroughEnv": [
    "NODE_TLS_REJECT_UNAUTHORIZED",
    "AWS_*",
    "SST_*"
  ],
  "tasks": {
    "build": {
      "persistent": false,
      "cache": true,
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        "dist/**",
        "build/**",
        ".next/**",
        "next-env.d.ts",
        ".expo/**",
        "_next/**",
        ".output/**",
        ".vercel/output/**",
        "**/.tsbuildinfo/**",
        "!**/.turbo/**",
        "!.next/cache/**"
      ]
    },
    "web#build": {
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "dependsOn": [
        "^build"
      ],
      "env": [
        "NEXT_PUBLIC_STATIC_URL"
      ],
      "outputs": [
        "dist/**",
        "build/**",
        ".next/**",
        "!.next/cache/**",
        "_next/**",
        "next-env.d.ts",
        ".expo/**",
        ".output/**",
        ".vercel/output/**",
        "**/.tsbuildinfo/**",
        "!**/.turbo/**"
      ]
    },
    "db:generate": {
      "dependsOn": [
        "^db:generate"
      ],
      "inputs": [
        "$TURBO_DEFAULT$"
      ]
    },
    "db:push": {
      "dependsOn": [
        "^db:push"
      ],
      "inputs": [
        "$TURBO_DEFAULT$"
      ],
      "cache": false,
      "interactive": true
    },
    "db:migrate": {
      "dependsOn": [
        "^db:migrate"
      ],
      "inputs": [
        "$TURBO_DEFAULT$"
      ]
    },
    "db:studio": {
      "dependsOn": [
        "^db:studio"
      ],
      "inputs": [
        "$TURBO_DEFAULT$"
      ]
    },
    "bootstrap": {
      "cache": false,
      "persistent": false
    },
    "lint": {
      "cache": true,
      "dependsOn": [
        "^lint"
      ],
      "inputs": [
        "$TURBO_DEFAULT$"
      ]
    },
    "lint:fix": {
      "cache": true,
      "dependsOn": [
        "^lint:fix"
      ],
      "inputs": [
        "$TURBO_DEFAULT$"
      ]
    },
    "lint:circular": {
      "cache": true,
      "dependsOn": [
        "^lint:circular"
      ],
      "inputs": [
        "$TURBO_DEFAULT$"
      ]
    },
    "knip": {
      "cache": true,
      "inputs": [
        "$TURBO_DEFAULT$",
        "knip.config.ts"
      ]
    },
    "check": {
      "dependsOn": [
        "^build",
        "^check"
      ],
      "inputs": [
        "$TURBO_DEFAULT$"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:https": {
      "cache": false,
      "persistent": true
    },
    "build:turbo": {
      "cache": true,
      "persistent": true
    },
    "gen:beep-paths": {
      "cache": false,
      "persistent": false
    },
    "iconify": {
      "cache": false,
      "persistent": false,
      "dependsOn": [
        "^build"
      ]
    },
    "start": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$"
      ]
    },
    "coverage": {
      "dependsOn": [
        "^test"
      ]
    },
    "docgen": {
      "cache": true,
      "dependsOn": ["^build"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "docgen.json",
        "src/**/*.ts"
      ],
      "outputs": [
        "docs/**"
      ]
    },
    "docgen:aggregate": {
      "cache": false,
      "dependsOn": ["docgen"],
      "inputs": [
        "**/docs/modules/**"
      ],
      "outputs": [
        "docs/**"
      ]
    },
    "docs:lint": {
      "cache": false,
      "dependsOn": [
        "^docs:lint"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        "packages/common/utils/src/**",
        "packages/common/identity/src/**",
        "packages/common/types/src/**",
        "packages/common/invariant/src/**",
        "tooling/repo-scripts/src/**"
      ],
      "outputs": [
        "jsdoc-analysis-results.json"
      ]
    },
    "docs:lint:file": {
      "cache": false,
      "dependsOn": [
        "^docs:lint:file"
      ],
      "inputs": [
        "$TURBO_DEFAULT$"
      ]
    },
    "docs:site": {
      "cache": false,
      "dependsOn": [
        "docgen"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        "packages/common/utils/docs/**",
        "packages/common/identity/docs/**",
        "packages/common/types/docs/**",
        "packages/common/invariant/docs/**"
      ],
      "outputs": [
        "docs/utils/**",
        "docs/types/**",
        "docs/invariant/**",
        "docs/identity/**"
      ]
    },
    "purge": {
      "cache": false
    },
    "ui-add": {
      "cache": false,
      "interactive": true
    }
  }
}
