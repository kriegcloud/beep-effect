{
  "private": true,
  "type": "module",
  "name": "@beep/root",
  "packageManager": "pnpm@10.15.0",
  "engines": {
    "pnpm": "10.15.0"
  },
  "workspaces": [
    "packages/*",
    "apps/*",
    "tooling/*"
  ],
  "repository": {
    "repository": {
      "type": "git",
      "url": "git@github.com:kriegcloud/beep-effect.git",
      "directory": "."
    }
  },
  "scripts": {
    "clean": "git clean -xdf node_modules .turbo",
    "clean:workspaces": "turbo run clean",
    "build": "pnpm dotenvx turbo run build",
    "build:turbo": "pnpm dotenvx turbo run build:turbo",
    "execute": "pnpm dotenvx turbo run execute",
    "check": "turbo run check",
    "check:all": "turbo run lint check test",
    "db:generate": "turbo run db:generate",
    "db:migrate": "pnpm dotenvx turbo run db:migrate",
    "db:push": "pnpm dotenvx turbo run db:push",
    "db:nuke": "docker compose down && docker system prune -a -f && docker volume prune -a -f",
    "db:exec": "pnpm dotenvx docker compose exec -it postgres '/bin/bash'",
    "db:studio": "pnpm dotenvx turbo run  db:studio",
    "db:up": "pnpm dotenvx docker compose up -d",
    "lint": "pnpm lint:deps && turbo run lint lint:circular",
    "lint:fix": "turbo run lint:fix",
    "test": "turbo run test",
    "coverage": "turbo run coverage",
    "dev:https": "pnpm dotenvx turbo run dev:https --concurrency=20",
    "dev": "pnpm dotenvx turbo run dev --concurrency=20",
    "start": "pnpm dotenvx turbo run start",
    "dotenvx": "dotenvx run -f .env --",
    "lint:deps": "pnpm syncpack lint --dependency-types=pnpmOverrides --dependencies='!{@tanstack/react-form,@effect/platform-node}'",
    "lint:deps:fix": "pnpm syncpack fix --dependency-types=pnpmOverrides --dependencies='!{@tanstack/react-form,@effect/platform-node}'",
    "deps:update": "pnpm syncpack update --target=latest --dependency-types=pnpmOverrides --dependencies='!{@tanstack/react-form,@effect/platform-node}'",
    "lint:circular": "turbo run lint:circular"
  },
