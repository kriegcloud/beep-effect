{
  "private": true,
  "type": "module",
  "name": "@beep/root",
  "packageManager": "bun@1.3.2",
  "workspaces": [
    "scratchpad",
    "packages/common/*",
    "packages/core/*",
    "packages/documents/*",
    "packages/iam/*",
    "packages/_internal/*",
    "packages/runtime/*",
    "packages/shared/*",
    "packages/ui/*",
    "apps/*",
    "tooling/*"
  ],
  "repository": {
    "repository": {
      "type": "git",
      "url": "git@github.com:kriegcloud/beep-effect.git",
      "directory": "."
    }
  },
  "scripts": {
    "init:env": "bun run tooling/cli/src/index.ts env",
    "purge": "bun run --cwd tooling/repo-scripts purge --",
    "build": "bunx turbo run build",
    "build:turbo": "bun run dotenvx -- bunx turbo run build:turbo",
    "execute": "bun run dotenvx -- bunx turbo run execute",
    "check": "bunx turbo run check",
    "check:all": "bunx turbo run lint check test",
    "db:generate": "bunx turbo run db:generate",
    "db:migrate": "bun run dotenvx -- bunx turbo run db:migrate",
    "db:push": "bun run dotenvx -- bunx turbo run db:push",
    "nuke": "docker compose down && docker system prune -a -f && docker volume prune -a -f",
    "services:up": "bun run dotenvx -- docker compose up -d",
    "db:exec": "bun run dotenvx -- docker compose exec -it postgres '/bin/bash'",
    "db:studio": "bun run dotenvx -- bunx turbo run db:studio",
    "lint": "bun run lint:deps && bunx turbo run lint lint:circular",
    "lint:fix": "bunx turbo run lint:fix && bun run lint:deps:fix",
    "test": "turbo run test --ui=stream",
    "coverage": "bunx turbo run coverage",
    "docs:lint": "bun run dotenvx -- bunx turbo run docs:lint",
    "docs:lint:file": "bun run dotenvx -- bunx turbo run docs:lint:file --",
    "docgen": "turbo run docgen",
    "docgen:init": "bun run tooling/cli/src/index.ts docgen init",
    "docgen:analyze": "bun run tooling/cli/src/index.ts docgen analyze",
    "docgen:generate": "bun run tooling/cli/src/index.ts docgen generate",
    "docgen:aggregate": "bun run tooling/cli/src/index.ts docgen aggregate",
    "docgen:status": "bun run tooling/cli/src/index.ts docgen status",
    "docgen:agents": "ANTHROPIC_API_KEY=$ANTHROPIC_API_KEY bun run tooling/cli/src/index.ts docgen agents",
    "dev:https": "bun run dotenvx -- bunx turbo run dev:https --concurrency=36",
    "dev": "bun run dotenvx -- bunx turbo run dev --concurrency=36",
    "bootstrap": "bunx tsx ./tooling/repo-scripts/src/bootstrap.ts",
    "start": "bun run dotenvx -- bunx turbo run start",
    "update-lockfile": "bun pm update",
    "dotenvx": "bunx @dotenvx/dotenvx run -f .env --",
    "lint:deps": "bunx syncpack lint --dependency-types='catalog' --dependencies='!{@tanstack/react-form}'",
    "lint:deps:fix": "bunx syncpack fix --dependency-types=catalog --dependencies='!{@tanstack/react-form}'",
    "deps:update": "bunx syncpack update --target=latest --dependency-types=catalog --dependencies='!{@tanstack/react-form}'",
    "lint:circular": "bunx turbo run lint:circular",
    "knip": "bunx knip --config=./knip.config.ts",
    "knip:ci": "bunx knip --config=./knip.config.ts --production",
    "knip:watch": "bunx knip --config=./knip.config.ts --watch --changes",
    "gen:secrets": "bun run dotenvx -- bunx tsx ./tooling/repo-scripts/src/generate-env-secrets.ts",
    "gen:beep-paths": "bun run dotenvx -- bunx tsx ./tooling/repo-scripts/src/generate-asset-paths.ts",
    "iconify": "bun run --cwd tooling/repo-scripts iconify",
    "ui-add": "turbo run ui-add",
    "prepare": "effect-language-service patch"
  },
  "resolutions#": {
    "@beep/*": "Needed to force PNPM to install local packages"
  },
  "overrides": {
    "slate": "0.117.0",
    "slate-dom": "0.117.4",
    "slate-react": "0.117.4"
  },
