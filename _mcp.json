{
  "mcpServers": {
    "next-devtools": {
      "command": "npx",
      "args": [
        "-y",
        "next-devtools-mcp@latest"
      ]
    },
    "effect_docs": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "timsmart/effect-mcp"
      ],
      "env": {}
    },
    "mui-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "@mui/mcp@latest"
      ]
    },
    "chrome-devtools": {
      "command": "/home/elpresidank/.nvm/versions/node/v20.19.0/bin/npx",
      "args": [
        "chrome-devtools-mcp@latest",
        "--connect-url"
      ]
    },
    "markdownify": {
      "command": "node",
      "args": [
        "/home/elpresidank/YeeBois/dev/ai-stuff/markdownify-mcp/dist/index.js"
      ]
    },
    "mcp-deepwiki": {
      "command": "npx",
      "args": [
        "-y",
        "@nekzus/mcp-server@latest"
      ]
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp",
        "--api-key",
        "${MCP_CONTEXT7_API_KEY}"
      ]
    },
    "zen": {
      "command": "bash",
      "args": [
        "-c",
        "for p in $(which uvx 2>/dev/null) $HOME/.local/bin/uvx /opt/homebrew/bin/uvx /usr/local/bin/uvx uvx; do [ -x \"$p\" ] && exec \"$p\" --from git+https://github.com/BeehiveInnovations/zen-mcp-server.git zen-mcp-server; done; echo 'uvx not found' >&2; exit 1"
      ],
      "env": {
        "PATH": "/usr/local/bin:/usr/bin:/bin:/opt/homebrew/bin:~/.local/bin",
        "GEMINI_API_KEY": "${MCP_GEMINI_API_KEY}",
        "OPENAI_API_KEY": "${MCP_OPENAI_API_KEY}",
        "XAI_API_KEY": "${MCP_XAI_API_KEY}",
        "DISABLED_TOOLS": "analyze,refactor,testgen,secaudit,docgen,tracer",
        "DEFAULT_MODEL": "auto"
      }
    }
  }
}